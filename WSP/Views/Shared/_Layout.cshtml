<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="~/Scripts/Controllers/ng-table.min.css" rel="stylesheet" />
    @*<link href="~/Scripts/Controllers/bootstrap.min.css" rel="stylesheet" />*@

    <script src="~/Scripts/Controllers/angular.min.js"></script>
    <script src="~/Scripts/Controllers/ui-bootstrap-tpls-1.3.2.js"></script>







</head>
<body>
    @RenderBody()

    <div class="container body-content">
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - DEREPA ALEXEY</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


    <!-- jQuery + Flot + Angular -->
    <script src="~/Scripts/flot/jquery.min.js"></script>
    <script src="~/Scripts/flot/jquery.flot.js"></script>
    <script src="~/Scripts/flot/angular.min.js"></script>


    <!-- Angular Flot + Example -->
    <script src="~/Scripts/flot/angular-flot.js"></script>


    <script src="~/Scripts/Controllers/SiteAddressController.js"></script>
</body>
</html>
